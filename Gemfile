source 'https://rubygems.org'

gem 'rails', '3.2.13'

gem 'bcrypt-ruby'
gem 'jquery-rails'
gem 'pg'

# Gems used only for assets and not required
# in production environments by default.
group :assets do
  gem 'sass-rails',   '~> 3.2.3'
  gem 'coffee-rails', '~> 3.2.1'
  gem 'uglifier', '>= 1.0.3'
end

group :development do
  gem 'annotate'
  gem 'better_errors'
  gem 'binding_of_caller'
  gem 'debugger'
  gem 'pry-rails'
  gem 'quiet_assets'
end